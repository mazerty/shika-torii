<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="fr.mazerty.shika.torii.ApplicationDao">

    <select id="selectByName" resultType="fr.mazerty.shika.ishi.Application">
        SELECT
            id,
            name
        FROM torii.t_application
        WHERE name = #{name}
    </select>

</mapper>
